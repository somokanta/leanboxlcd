function leanbox_dashboard_form_alter(&$form, &$form_state, $form_id) {
  global $user;
  $roles = $user->roles;
  $rs_operator_role_id = get_role_by_name('RS Operator');
  $dfe_role_id = get_role_by_name('DFE');
  $rs_admin_role_id = get_role_by_name('RS Admin');


  if ($form_id == 'user_register_form' || $form_id == 'user_profile_form') {

    if (in_array("hul_admin", $roles)) {
      $form['#validate'][] = 'leanbox_dashboard_user_register_validate';

      $role_names = array('RS Operator', 'DFE', 'RS Admin');
      foreach ($form['account']['roles']['#options'] as $rid => $rname) {
        if (!in_array($rname, $role_names)) {
          unset($form['account']['roles']['#options'][$rid]);
        }
      }

      $form['field_name']['#states'] = array(
        'visible' => array(
          ':input[name="roles['. $rs_operator_role_id . ']"]' => array('checked' => TRUE),
        ),
