        . '<li>UNLOADING SUPERVISOR</li></Ol>';
    $form['feeds']['FeedsCSVParser']['help'][] = array(
      '#markup' => $list,
    );
  }
}

/**
 * Validate Handler for user_register_form
 * @param type $form
 * @param array $form_state
 */
function leanbox_dashboard_user_register_validate($form, &$form_state) {
  $roles = isset($form_state['values']['roles']) ? $form_state['values']['roles'] : '';
  $role_id = get_role_by_name('RS Operator');

  if (!empty($roles) && !empty($roles[$role_id])) {
    if (isset($form_state['values']['field_name']['und'][0]['value']) && empty($form_state['values']['field_name']['und'][0]['value'])) {
      form_set_error('field_name', t('kindly Enter RS Name'));
    }
    else {
      if (isset($form_state['values']['field_name']['und'][0]['value']) && (!preg_match('/^[a-zA-Z\s]+$/', $form_state['values']['field_name']['und'][0]['value']))) {
        form_set_error('field_name', t('Alphanumeri or Integer values not allowed in RS Name'));
      }
    }
    if (isset($form_state['values']['field_third_party_username']['und'][0]['value'])) {
      $rs_code = $form_state['values']['field_third_party_username']['und'][0]['value'];
      if (empty($rs_code)) {
        form_set_error('field_third_party_username', t('kindly Enter RS Code'));
      }
      else {
        $query = db_select('field_data_field_third_party_username', 'tp');
        $query->fields('tp', array('entity_id'));
        $query->condition('tp.field_third_party_username_value', $rs_code);
        $res = $query->execute()->fetchField();
        if (!empty($res)) {
          form_set_error('field_third_party_username', t('This RS Code is already taken.Kindly Enter another RS Code'));
        }
      }
    }
  }
  if (isset($form_state['values']['field_distributor_custom'])) {
    $form_state['values']['field_distributor']['und'][0]['value'] = $form_state['values']['field_distributor_custom'];
  }
}

function leanbox_dashboard_van_stop_master($field_name) {
    $query = db_select('node', 'n');
    $query->addTag('node_uid');
    $query->condition('n.type', 'van_stop_master');
    $query->join("field_data_". $field_name, 'pp', 'pp.entity_id = n.nid');
    $query->fields('pp', array($field_name. "_value"));
    $dis_time = $query->execute()->fetchField();
    return $dis_time;
}


