         }
         $form['field_third_party_username_value']['#type'] = 'select';
         $form['field_third_party_username_value']['#options'] = $res;
         $form['field_third_party_username_value']['#multiple'] = TRUE;
         $form['field_third_party_username_value']['#size'] = 30;
         $form['field_third_party_username_value']['#attributes'] = array('style' => array('height:6em;'), 'class' => array('class-rs-code'));
         $form['field_third_party_username_value']['#default_value'] = $rs_default_value;
      }

      // party packing
    if (isset($form['field__party_packingstatus_value'])) {
      $form['field__party_packingstatus_value']['#options']['empty'] = t('Empty');
    }
     if (isset($form['field__party_packingstatus_value_op'])) {
      $form['field__party_packingstatus_value_op']['#options'] = array('or' => 'Is One Of');
