      $form['field_distributor_custom']['#states'] = array(
        'visible' =>
        array(
          array(
            ':input[name="roles['. $dfe_role_id . ']"]' => array('checked' => TRUE),
          ),
          array(
            ':input[name="roles['. $rs_admin_role_id . ']"]' => array('checked' => TRUE),
          )
