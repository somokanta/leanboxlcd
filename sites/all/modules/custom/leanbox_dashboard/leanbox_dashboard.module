         $query->leftjoin('field_data_field_name', 'n', 'n.entity_id = t.entity_id');
         $query->addExpression("concat(t.field_third_party_username_value ,' : ', n.field_name_value)", 'name');
         if (!empty($values)) {
           $query->condition('t.entity_id', $values, 'IN');
         }
         $res = $query->execute()->fetchAllKeyed(0, 1);
         $res[''] = t('-Any-');
      
         if (isset($_GET['field_third_party_username_value'])) {
